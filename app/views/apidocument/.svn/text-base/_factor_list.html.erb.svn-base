  <script type="text/javascript">
      function change_FactorList()
      {
          var link = '<%= "#{DOMAIN_CONFIG}"+api_v1_factor_index_path
%>?';
                          link += "auth_token=" + escape($("#input_FactorList_token").val()) + "&";
                          $("#example_FactorList").val(link);
      }
